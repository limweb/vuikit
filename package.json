{
  "name": "vuikit",
  "version": "0.7.0",
  "description": "UIkit with all the power of Vue",
  "author": {
    "name": "ZOOlanders",
    "url": "http://www.zoolanders.com"
  },
  "keywords": [
    "library",
    "vue",
    "vuejs",
    "uikit",
    "vuikit"
  ],
  "main": "dist/vuikit.js",
  "module": "dist/vuikit.mjs",
  "jsnext:main": "dist/vuikit.mjs",
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT",
